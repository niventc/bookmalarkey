{
    "manifest_version": 2,
    "name": "bookmalarkey",
    "description": "",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html"
    },    
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    // TODO remove when AOT
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}